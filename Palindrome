Leet code : https://leetcode.com/problems/palindrome-number

// very basic question

class Solution {
public:
    bool isPalindrome(int x) {
        
        long rev=0;
        int temp = x;
        while(x>0)
        {
            int r = x%10;
            rev = rev*10+ r;
            x= x/10;
        }
        if(rev == temp)
        {
            return true;
        }
        return false;
        
    }
};
